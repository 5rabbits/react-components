{
  "name": "@5rabbits/time-input",
  "version": "0.2.2",
  "main": "dist/time-input.js",
  "files": ["dist/*"],
  "license": "MIT",
  "description": "A time input in [00h 00m] format.",
  "author": "5rabbits <5rabbits@lemontech.com> (http://www.5rabbits.com)",
  "keywords": ["time", "input", "5rabbits", "lemontech", "react"],
  "repository": {
    "type": "git",
    "url": "https://github.com/5rabbits/time-input"
  },
  "publishConfig": {
    "access": "public"
  },
  "homepage": "https://www.lemontech.com",
  "bugs": "https://github.com/5rabbits/time-input/issues",
  "scripts": {
    "build": "rollup -c",
    "start":
      "concurrently \"rollup -c -w\" \"start-storybook -p 9001\" --names \"rollup,storybook\" --kill-others",
    "prepublish": "yarn build",
    "postpublish": "yarn deploy-storybook",
    "lint": "eslint . --ext .js,.jsx",
    "prettify": "prettier \"**/*.{js,json,css,md}\" --write",
    "deploy-storybook": "storybook-to-ghpages",
    "test": "jest"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,json,css,md}": ["prettier --write", "git add"],
    "*.js": ["eslint"]
  },
  "devDependencies": {
    "@5rabbits/eslint-config": "1.0.2",
    "@storybook/addon-actions": "3.3.15",
    "@storybook/react": "3.3.15",
    "@storybook/storybook-deployer": "2.3.0",
    "babel-core": "6.26.0",
    "babel-eslint": "8.2.2",
    "babel-jest": "23.0.0-alpha.0",
    "babel-plugin-external-helpers": "6.22.0",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-plugin-transform-decorators-legacy": "1.3.4",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-preset-env": "1.6.1",
    "babel-preset-react": "6.24.1",
    "concurrently": "3.5.1",
    "enzyme": "3.3.0",
    "enzyme-adapter-react-15": "1.0.5",
    "enzyme-to-json": "3.3.3",
    "eslint": "^4.9.0",
    "eslint-config-airbnb": "16.1.0",
    "eslint-config-prettier": "2.9.0",
    "eslint-import-resolver-webpack": "0.8.4",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.4.0",
    "husky": "0.15.0-rc.8",
    "identity-obj-proxy": "3.0.0",
    "jest": "22.4.2",
    "jest-enzyme": "6.0.0",
    "lint-staged": "7.0.0",
    "postcss-import": "11.1.0",
    "postcss-nested": "3.0.0",
    "postcss-sass-color-functions": "1.1.0",
    "postcss-sassy-mixins": "2.1.0",
    "postcss-simple-vars": "4.1.0",
    "prettier": "1.11.0",
    "react": "^15.0.0",
    "react-dom": "^15.0.0",
    "react-test-renderer": "^15.0.0",
    "rollup": "0.56.3",
    "rollup-plugin-babel": "3.0.3",
    "rollup-plugin-cleaner": "0.2.0",
    "rollup-plugin-commonjs": "8.3.0",
    "rollup-plugin-filesize": "1.5.0",
    "rollup-plugin-includepaths": "0.2.2",
    "rollup-plugin-json": "2.3.0",
    "rollup-plugin-node-resolve": "3.0.3",
    "rollup-plugin-postcss": "1.3.1",
    "rollup-plugin-replace": "2.0.0",
    "rollup-plugin-svg-to-jsx": "1.0.0",
    "rollup-plugin-visualizer": "0.3.1",
    "storybook-readme": "3.3.0"
  },
  "peerDependencies": {
    "react": "^15.0.0 || ^16.0.0",
    "react-dom": "^15.0.0 || ^16.0.0"
  },
  "dependencies": {
    "prop-types": "^15.5.0"
  },
  "jest": {
    "moduleDirectories": ["src", "node_modules"],
    "moduleNameMapper": {
      "\\.css$": "identity-obj-proxy",
      "\\.svg$": "identity-obj-proxy"
    },
    "collectCoverage": true,
    "coverageDirectory": "tests/coverage",
    "coveragePathIgnorePatterns": ["node_modules", "tests/setup.js"],
    "coverageReporters": ["html", "text-summary", "lcov"],
    "snapshotSerializers": ["<rootDir>/node_modules/enzyme-to-json/serializer"],
    "setupTestFrameworkScriptFile": "<rootDir>/tests/setup.js"
  }
}
